	},
	"scripts": {
		"barrel": "tsx ./scripts/barrels.ts",
		"build": "tsx ./scripts/build-packages.ts",
		"build:integration": "tsx ./scripts/build-integration.ts",
		"clean": "rimraf ../build && rimraf ../dist && rimraf ../integration",
		"doc": "tsx ./scripts/simple-readme.ts",
		"lint": "eslint ..",
		"lint:fix": "eslint --fix .",
		"precommit": "npm run lint && npm run barrel && npm run type-check && npm run build && npm run build:integration && npm run test && npm run tstyche && npm run pub",
		"pub": "tsx ./scripts/publish.ts",
		"pub:commit": "tsx ./scripts/publish.ts --commit",
		"reinstall": "rimraf ./node_modules && npm ci",
		"reinstall:full": "rimraf ./node_modules package-lock.json && npm install",
		"release": "tsx scripts/bump-version.ts --dry-run",
		"release:commit": "tsx scripts/bump-version.ts",
		"test": "mocha",
		"test:coverage": "nyc mocha",
		"tstyche": "tstyche packages",
		"type-check": "tsc --noEmit",
		"upgrade:check": "npm-check-updates"
	},
	"version": "2025.2.16"
