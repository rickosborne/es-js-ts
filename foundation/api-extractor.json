{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"apiReport": {
		"enabled": false,
		"reportFolder": "<projectFolder>/dist/",
		"reportTempFolder": "<projectFolder>/build/api/",
		"reportVariants": [
			"public"
		]
	},
	"bundledPackages": [
		"@rickosborne/*"
	],
	"docModel": {
		"apiJsonFilePath": "<projectFolder>/build/api/api.json",
		"enabled": true,
		"projectFolderUrl": "https://github.com/rickosborne/es-js-ts/tree/main/foundation"
	},
	"dtsRollup": {
		"enabled": false
	},
	"mainEntryPointFilePath": "<projectFolder>/dist/types/index.d.ts",
	"messages": {
		"extractorMessageReporting": {
			"ae-forgotten-export": {
				"logLevel": "none"
			},
			"ae-missing-release-tag": {
				"logLevel": "none"
			},
			"ae-wrong-input-file-type": {
				"logLevel": "none"
			}
		}
	},
	"newlineKind": "lf",
	"tsdocMetadata": {
		"tsdocMetadataFilePath": "<projectFolder>/dist/tsdoc-metadata.json"
	}
}
