{
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"apiReport": {
		"enabled": false,
		"reportFolder": "<projectFolder>",
		"reportTempFolder": "<projectFolder>/../../build/${moduleName}/api",
		"reportVariants": [
			"public"
		]
	},
	"bundledPackages": [
		"@rickosborne/*"
	],
	"compiler": {
		"tsconfigFilePath": "<projectFolder>/../../src/tsconfig.build.json"
	},
	"docModel": {
		"apiJsonFilePath": "<projectFolder>/../../build/${moduleName}/api/api.json",
		"enabled": true,
		"projectFolderUrl": "https://github.com/rickosborne/es-js-ts/tree/main/src/packages/${moduleName}"
	},
	"dtsRollup": {
		"enabled": false
	},
	"mainEntryPointFilePath": "<projectFolder>/index.d.ts",
	"messages": {
		"compilerMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		},
		"extractorMessageReporting": {
			"ae-forgotten-export": {
				"logLevel": "none"
			},
			"ae-incompatible-release-tags": {
				"logLevel": "none"
			},
			"ae-internal-missing-underscore": {
				"logLevel": "none"
			},
			"ae-missing-release-tag": {
				"logLevel": "none"
			},
			"ae-wrong-input-file-type": {
				"logLevel": "none"
			},
			"default": {
				"logLevel": "warning"
			}
		},
		"tsdocMessageReporting": {
			"default": {
				"logLevel": "warning"
			}
		}
	},
	"newlineKind": "lf",
	"projectFolder": "../../dist/${moduleName}",
	"tsdocMetadata": {
		"tsdocMetadataFilePath": "<projectFolder>/../../build/${moduleName}/tsdoc-metadata.json"
	}
}
