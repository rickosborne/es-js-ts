{
	"author": {
		"name": "Rick Osborne",
		"url": "https://rickosborne.org"
	},
	"description": "Rick Osborne's JS/ES/TS Tools",
	"devDependencies": {
		"@eslint/js": "8.57.1",
		"@stylistic/eslint-plugin-js": "1.8.1",
		"@tsconfig/node22": "22.0.0",
		"@types/chai": "4.3.16",
		"@types/eslint__js": "8.42.3",
		"@types/mocha": "10.0.10",
		"@types/node": "22.10.2",
		"@typescript-eslint/eslint-plugin": "6.21.0",
		"@typescript-eslint/parser": "6.21.0",
		"chai": "4.5.0",
		"eslint": "8.57.1",
		"eslint-plugin-editorconfig": "4.0.3",
		"eslint-plugin-jsonc": "2.18.2",
		"eslint-plugin-markdown": "5.1.0",
		"eslint-plugin-require-extensions": "0.1.3",
		"eslint-plugin-sort-keys-fix": "1.1.2",
		"globals": "15.14.0",
		"mocha": "11.0.1",
		"npm-check-updates": "17.1.12",
		"nyc": "17.1.0",
		"source-map-support": "0.5.21",
		"tsx": "4.19.2",
		"typescript": "5.7.2"
	},
	"engines": {
		"node": ">=14"
	},
	"exports": {
		".": {
			"types": "./dist/types/index.d.ts",
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js",
			"default": "./dist/esm/index.js"
		}
	},
	"files": [
		"dist"
	],
	"homepage": "https://github.com/rickosborne/es-js-ts",
	"license": "CC-BY-NC-SA-4.0",
	"main": "dist/cjs/index.js",
	"name": "@rickosborne/js-es-ts",
	"private": true,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rickosborne/es-js-ts.git"
	},
	"scripts": {
		"build-all": "tsx scripts/build-all.ts",
		"check-exports": "tsx scripts/check-exports.ts",
		"lint": "eslint .",
		"reinstall": "rm -rf ./node_modules && npm ci",
		"reinstall:full": "rm -rf ./node_modules package-lock.json && npm install",
		"test": "mocha",
		"test:coverage": "nyc mocha",
		"type-check": "tsc --noEmit",
		"upgrade:check": "npm-check-updates"
	},
	"version": "2024.12.23"
}
